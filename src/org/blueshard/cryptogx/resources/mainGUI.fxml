            <Image url="@minimize.png" />
         </image></ImageView>
      <ImageView fx:id="closeWindow" fitHeight="25.0" fitWidth="25.0" layoutX="875.0" onMouseClicked="#closeApplication" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@close.png" />
         </image></ImageView>
